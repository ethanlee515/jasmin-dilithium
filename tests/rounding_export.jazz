require "../src/high-low-order.jazz"

export
fn decompose_jazz(reg u32 a, reg u64 pa0, reg u64 pa1)
{
	reg u32 a0;
	reg u32 a1;
	a0, a1 = decompose(a);
	(u32)[pa0] = a0;
	(u32)[pa1] = a1;
}

export
fn make_hint_jazz(reg u32 a0, reg u32 a1) -> reg u8
{
	reg u8 result;
	result = make_hint(a0, a1);
	return result;
}
