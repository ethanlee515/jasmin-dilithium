require "../src/gen_matrix.jazz"

export
fn gen_matrix_entry_test(reg u64 out) {
	stack u8[32] rho;

	stack u32[Li2_polydeg] buf;
	reg u32 y;
	inline int i;

	for i = 0 to 32 {
		rho[i] = 0;
	}

	buf = gen_matrix_entry(rho, 3, 4, buf);

	for i = 0 to Li2_polydeg {
		y = buf[i];
		(u32) [out + 4 * i] = y;
	}
}
